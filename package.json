{
  "name": "enonic-fp",
  "version": "0.2.12",
  "description": "Functional programming helpers for Enonic XP",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "npm run clean && tsc",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepublish": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItemConsulting/enonic-fp.git"
  },
  "keywords": [
    "enonic",
    "fp"
  ],
  "author": "Tom Arild Jakobsen <tajakobsen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItemConsulting/enonic-fp/issues"
  },
  "homepage": "https://github.com/ItemConsulting/enonic-fp#readme",
  "dependencies": {
    "enonic-types": "^0.0.15",
    "fp-ts": "^2.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix 'src/**/*.ts'",
      "git add"
    ]
  }
}
