{
  "name": "enonic-fp",
  "version": "0.1.4",
  "description": "Functional programming helpers for Enonic XP",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "npm run clean && tsc",
    "lint:ts": "tslint --project tsconfig.json --config tslint.json --fix",
    "prettier:ts": "prettier --write 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItemConsulting/enonic-fp.git"
  },
  "keywords": [
    "enonic",
    "fp"
  ],
  "author": "Tom Arild Jakobsen <tajakobsen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItemConsulting/enonic-fp/issues"
  },
  "homepage": "https://github.com/ItemConsulting/enonic-fp#readme",
  "dependencies": {
    "fp-ts": "^2.1.0"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "prettier": "1.18.2",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --project tsconfig.json --config tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  }
}
