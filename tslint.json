{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": [true, "never-prefix"],
      "array-type": [true, "generic"],
      "jsdoc-format": [true, "check-multiline-start"],
      "trailing-comma": [true, {
        "multiline": "never",
        "esSpecCompliant": true
      }]
    },
    "rulesDirectory": []
}
